    </html><br><br><h1>Listing correspondences</h1>

<table border="1" style="width:100%">
 
  <tr>
    <th>Type of corr</th>
    <th>Subject</th>
    <th>User initials</th>
    <th>Medium</th>
    <th>Address</th>
    <th>Description</th>
    <th>Date</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @correspondences.each do |correspondence| %>
  <tr>
    <td><%= correspondence.type_of_corr %></td>
    <td><%= correspondence.subject %></td>
    <td><%= correspondence.user_initials %></td>
    <td><%= correspondence.medium %></td>
    <td><%= correspondence.address %></td>
    <td><%= correspondence.description %></td>
    <td><%= correspondence.date %></td>
    <td><%= link_to 'Show', correspondence %></td>
    <td><%= link_to 'Edit', edit_correspondence_path(correspondence) %></td>
    <td><%= link_to 'Destroy', correspondence, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

